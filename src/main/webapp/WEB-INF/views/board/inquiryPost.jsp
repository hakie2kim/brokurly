<script>

    // 유형 메뉴 드롭다운
    function popup() {
        const menuDiv = document.getElementById('menu');

        if (menuDiv.style.display === 'none' || menuDiv.style.display === '') {
            menuDiv.style.display = 'block';
        } else {
            menuDiv.style.display = 'none';
        }
    }

    function popupDetail() {
        const menuDiv = document.getElementById('menuDetail');

        if (menuDiv.style.display === 'none' || menuDiv.style.display === '') {
            menuDiv.style.display = 'block';
        } else {
            menuDiv.style.display = 'none';
        }
    }



    // 제목 placeholder

    document.addEventListener('DOMContentLoaded', () => {
        const input = document.getElementById('inquiry-subject');
        const placeholderText = '제목을 입력해주세요'; // Store the placeholder text

        input.addEventListener('focus', function() {
            this.setAttribute('placeholder', ''); // Clear placeholder on focus
        });

        input.addEventListener('blur', function() {
            if (this.value === '') {
                this.setAttribute('placeholder', placeholderText); // Restore placeholder if no text entered
            }
        });
    });



    // 내용 placeholder
    // textarea에 class 이름에만 있는 placeholder 기능구현
    const textarea = document.getElementById('inquiry-contents');
    const placeholderDiv = document.querySelector('.placeholder.css-1ofzfyp.e1tjt2bn6');

    // Function to toggle placeholder visibility based on textarea content
    function toggleCustomPlaceholder() {
        if (textarea.value.length > 0) {
            placeholderDiv.style.display = 'none';
        } else {
            placeholderDiv.style.display = 'block';
        }
    }

    // Event listener for input on textarea
    textarea.addEventListener('input', toggleCustomPlaceholder);

    // Initial check in case there's already text in the textarea (e.g., browser autofill)
    toggleCustomPlaceholder();

    // 글자수 카운터
    const characterCounter = document.querySelector('.content-length-counter .css-14kcwq8.e1tjt2bn2');
    let lastValidValue = ''; // Store the last valid content

    textarea.addEventListener('input', function() {
        const currentLength = this.value.length;
        if (currentLength <= 5000) {
            // Update the character count display
            characterCounter.textContent = `${currentLength + ''} 자`;
            lastValidValue = this.value; // Update the last valid content
        } else {
            // If character limit is exceeded
            alert('문의는 5000자 이하로 남겨주세요');
            this.value = lastValidValue; // Revert to the last valid content
            characterCounter.textContent = `${lastValidValue.length} 자`; // Correct the character count display
        }
    });


    // 등록 버튼 조건
    document.addEventListener('PostConditionCheck', function() {
        const submitButton = document.querySelector('.css-114fr9p ebvrvv10');
        const subjectInput = document.getElementById('inquiry-subject');
        const contentsTextarea = document.getElementById('inquiry-contents');

        function checkInputs() {
            // 입력값 검사
            const isSubjectFilled = subjectInput.value.trim() !== '';
            const isContentsFilled = contentsTextarea.value.trim() !== '';

            // 모든 필드가 채워져 있으면 버튼 활성화 및 색상 변경
            if (isSubjectFilled && isContentsFilled) {
                submitButton.disabled = false;
                submitButton.style.backgroundColor = 'rgb(17, 150, 127)';
                console.log("on")
            } else {
                submitButton.disabled = true;
                submitButton.style.backgroundColor = ''; // 기본 색상 (또는 비활성화 색상)
                console.log("on")
            }
        }

        // 'input' 이벤트 리스너 등록
        subjectInput.addEventListener('input', checkInputs);
        contentsTextarea.addEventListener('input', checkInputs);

        // 페이지 로딩 시 초기 상태 확인
        checkInputs();
    });
</script>
